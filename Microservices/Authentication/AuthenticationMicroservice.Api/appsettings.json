{
  "AppSettings": {
    "RefreshTokenInDaysTTL": 7,
    "RefreshTokenInDaysForDbTTL": 2,
    "TokenInMinutesTTL": 60
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    //"QueriesConnectionString": "Server=(localdb)\\MSSQLLocalDB;Database=TeamHikerDB;",
    //"CommandsConnectionString": "Server=(localdb)\\MSSQLLocalDB;Database=TeamHikerDB;"
    //"QueriesConnectionString": "Data Source=.;Initial Catalog=TeamHikerDB;User ID=sa;Password=team@Hiker#2024!;Encrypt=True;Trust Server Certificate=True;",
    //"CommandsConnectionString": "Data Source=.;Initial Catalog=TeamHikerDB;User ID=sa;Password=team@Hiker#2024!;Encrypt=True;Trust Server Certificate=True;",
    "DefaultConnection": "Server=.;User ID=sa;Password=team@Hiker#2024!;Database=TeamHikerDB;MultipleActiveResultSets=true;TrustServerCertificate=True;"
  },
  "QueriesDatabaseProvider": 0,
  "CommandsDatabaseProvider": 0,
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": "Warning",
    "Override": {
      "Microsoft": "Warning",
      "System": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
      "Microsoft.*": "Warning"
    },
    "WriteTo:Async": {
      "Name": "Async",
      "Args": {
        "configure": [
          {
            "Name": "File",
            "Args": {
              "path": "logs/log-.log",
              "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}",
              "rollingInterval": "Day",
              "shared": true
            }
          }
        ]
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.log",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}",
          "rollingInterval": "Day",
          "shared": true
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Sample"
    }
  }
}
